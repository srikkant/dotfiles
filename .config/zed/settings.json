{
    "assistant": {
        "button": false,
        "version": "2",
        "default_model": {
            "provider": "copilot_chat",
            "model": "gpt-4o"
        }
    },
    "buffer_font_fallbacks": ["Symbols Nerd Font Mono"],
    "buffer_font_size": 14,
    "chat_panel": {
        "button": false
    },
    "collaboration_panel": {
        "button": false,
        "dock": "right"
    },
    "languages": {
        "CSS": {
            "formatter": "prettier"
        },
        "HTML": {
            "formatter": "prettier"
        },
        "JavaScript": {
            "formatter": "prettier",
            "code_actions_on_format": {
                "source.fixAll.eslint": true,
                "source.organizeImports": true
            }
        },
        "TypeScript": {
            "formatter": "prettier",
            "code_actions_on_format": {
                "source.fixAll.eslint": true,
                "source.organizeImports": true
            }
        },
        "TSX": {
            "formatter": "prettier",
            "code_actions_on_format": {
                "source.fixAll.eslint": true,
                "source.organizeImports": true
            }
        }
    },
    "notification_panel": {
        "button": false
    },
    "outline_panel": {
        "dock": "right",
        "button": false
    },
    "relative_line_numbers": true,
    "scrollbar": {
        "show": "never"
    },
    "scroll_beyond_last_line": "off",
    "tab_bar": {
        "show": false
    },
    "tab_size": 4,
    "terminal": {
        "toolbar": {
            "title": false
        },
        "line_height": "standard",
        "button": false
    },
    "theme": {
        "dark": "Rosé Pine",
        "light": "Rosé Pine Dawn",
        "mode": "system"
    },
    "toolbar": {
        "quick_actions": false
    },
    "vertical_scroll_margin": 0,
    "vim_mode": true,
    "vim": {
        "use_system_clipboard": "always"
    }
}
